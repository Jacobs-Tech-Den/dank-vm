# CollabVM Server mkconfig for GNU/Linux

WIN32=0

CXX = clang++
CC = clang
ODB = odb

LIBS = -pthread -lturbojpeg -ljpeg -lboost_system -lpng -lz -lvncclient -lcairo -lpixman-1 -ldl -lsqlite3

OBJDIR = obj
BINDIR = bin

ifeq ($(EXECINFO),1)
LIBS += -lexecinfo
endif

.PHONY: all pre clean
.SUFFIXES: .o .cpp

-include mk/Makedefs

